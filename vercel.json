{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next@14.2.4"
    }
  ],
  "build": {
    "env": {
      "NEXT_PRIVATE_TARGET": "server",
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "installCommand": "npm ci --prefer-offline --legacy-peer-deps",
  "framework": "nextjs",
  "nodeVersion": "18.x"
}