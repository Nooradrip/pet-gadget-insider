{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next@14.2.4"
    }
  ],
  "build": {
    "env": {
      "NEXT_PRIVATE_TARGET": "server",
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "installCommand": "npm install --legacy-peer-deps --force",
  "framework": "nextjs",
  "nodeVersion": "18.x"
}